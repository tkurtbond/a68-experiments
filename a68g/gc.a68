# I *think* this exercises the garbage collection.  #
BEGIN
  LONG LONG INT number of iterations;
  number of iterations := 0;
  WHILE TRUE DO
    STRING s = "";
    number of iterations +:= 1;
    IF (number of iterations MOD 1 000 000) = 0
    THEN print ((number of iterations, " ", new line))
    FI
  OD
END
    
